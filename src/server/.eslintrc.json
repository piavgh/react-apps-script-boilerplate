{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "google",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "googleappsscript",
    "prettier"
  ],
  "env": {
    "googleappsscript/googleappsscript": true
  },
  "globals": {
    "FormApp": true,
    "HtmlService": true,
    "global": true,
    "console": true,
    "process": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "semi": [
      2,
      "never"
    ],
    "no-console": 0,
    "no-unused-expressions": 0
  }
}
